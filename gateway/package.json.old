{
  "name": "ms-gateway",
  "version": "2.0.0",
  "description": "Alt default gateway microservice",
  "author": "florian <florian@alt.bzh>",
  "license": "ISC",
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "tsc && npx tsc-alias && npm run copy-certs",
    "copy-certs": "cp -r ./src/certs/ ./dist/",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev"
  },
  "dependencies": {
    "@fastify/cookie": "^6.0.0",
    "@fastify/cors": "^7.0.0",
    "@fastify/helmet": "^8.1.0",
    "@fastify/http-proxy": "^7.1.0",
    "@fastify/jwt": "^5.0.1",
    "@fastify/multipart": "^6.0.0",
    "@fastify/redis": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/http-errors": "^1.8.2",
    "@types/ioredis": "^4.28.10",
    "@types/negotiator": "^0.6.1",
    "abstract-cache": "^1.0.1",
    "abstract-cache-redis": "^2.0.0",
    "body-parser": "^1.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "config": "^3.3.7",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.1",
    "envalid": "^7.3.1",
    "fastify": "^3.29.0",
    "fastify-healthcheck": "^3.1.0",
    "fastify-raw-body": "^3.2.0",
    "http-errors": "^2.0.0",
    "ioredis": "^5.0.5",
    "nanoid": "^3.3.4",
    "nats": "^2.7.1",
    "negotiator": "^0.6.3",
    "npm-check-updates": "^16.6.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
